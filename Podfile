# platform :ios, '9.0'

target 'Twitter' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Twitter
pod 'AFNetworking', '3.0.0'
pod 'BDBOAuth1Manager', '2.0.0'
pod 'KLCPopup'
pod 'NMPopUpViewSwift'
    # Pods for testing
  target 'TwitterTests' do
    inherit! :search_paths
    # Pods for testing
pod 'AFNetworking', '3.0.0'
pod 'BDBOAuth1Manager', '2.0.0'
pod 'NMPopUpViewSwift'
  end

  target 'TwitterUITests' do
    inherit! :search_paths
    # Pods for testing
pod 'AFNetworking', '3.0.0'
pod 'BDBOAuth1Manager', '2.0.0'
pod 'NMPopUpViewSwift'
  end

end
